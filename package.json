{
  "name": "@becomes/cms-ui",
  "version": "2.0.71",
  "description": "BCMS UI.",
  "author": "becomes.co",
  "scripts": {
    "start": "sirv public -s --host 0.0.0.0",
    "serve": "npm run start -- --dev",
    "dev": "node bin/bcms-ui.js --dev --isolated",
    "local:build": "rollup -c",
    "local:dev": "rollup -c -w",
    "local:check": "svelte-check --fail-on-warnings --fail-on-hints --workspace src",
    "local:lint": "eslint ./src --ext .ts",
    "build": "node tools.js --build",
    "bundle": "node tools.js --bundle",
    "link": "node tools.js --link",
    "unlink": "node tools.js --unlink",
    "publish:dist": "node tools.js --publish",
    "test": "node test.js"
  },
  "devDependencies": {
    "@becomes/cms-backend": "^2.0.46",
    "@becomes/cms-bundler": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "fs-extra": "^9.1.0"
  },
  "dependencies": {
    "@becomes/cms-sdk": "^2.0.21",
    "@becomes/svelte-router": "^1.0.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/highlight.js": "^10.1.0",
    "@types/node": "^14.17.1",
    "@types/quill": "^2.0.7",
    "@types/remarkable": "^2.0.1",
    "@types/simplemde": "^1.11.7",
    "@types/uuid": "^8.3.0",
    "@uppy/core": "^1.18.1",
    "@uppy/dashboard": "^1.19.1",
    "@uppy/image-editor": "^0.2.4",
    "arg": "^5.0.0",
    "autoprefixer": "^10.2.6",
    "gsap": "^3.6.1",
    "highlight.js": "^10.7.2",
    "node-sass": "^5.0.0",
    "postcss": "^8.3.0",
    "quill": "^1.3.7",
    "remarkable": "^2.0.1",
    "rollup": "^2.50.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sirv-cli": "^1.0.12",
    "svelte": "^3.38.2",
    "svelte-check": "^1.6.0",
    "svelte-preprocess": "^4.7.3",
    "tslib": "^2.2.0",
    "typescript": "^3.9.9",
    "uuid": "^8.3.2"
  },
  "bin": {
    "bcms-ui": "bin/bcms-ui.js"
  },
  "keywords": [
    "bcms",
    "svelte"
